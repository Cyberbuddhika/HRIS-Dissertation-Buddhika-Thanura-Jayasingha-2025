    <h2 class="text-xl font-semibold mb-4">Timesheets Summary </h2>

    <!-- Timesheet Summary Table -->
    <table id="timesheetTable" class="w-full text-sm table-auto border-collapse border border-gray-300">
  <thead id="timesheetTableHead">
    <tr>
      <th class="border px-4 py-2" rowspan="2">Consultant Name</th>
      <th class="border px-4 py-2" rowspan="2">Total Worked Hours</th>
      <th class="border px-4 py-2" rowspan="2">Total Worked Days</th>
      <th id="milestoneHeader" class="border px-4 py-2" colspan="1">Deliverable Summary</th>
      <th class="border px-4 py-2 w-[180px]" rowspan="2">Leave Summary</th>
    </tr>
    <tr>
      {{#each milestoneHeaders}}
        <th class="border px-4 py-2">{{this}}</th>
      {{/each}}
    </tr>
  </thead>
  <tbody id="timesheetTableBody">
    {{#each consultants}}
      <tr>
        <td class="border px-4 py-2">{{this.consultantName}}</td>
        <td class="border px-4 py-2">{{this.workedHours}} hrs</td>
        <td class="border px-4 py-2">{{this.workedDays}} days</td>
        {{#each ../milestoneHeaders}}
          <td class="border px-4 py-2">
            {{lookup ../this.milestoneSummary this}} hrs
          </td>
        {{/each}}
        <td class="border px-4 py-2">
          {{#each this.leaveSummary}}
            {{@key}}: {{this}} days <br>
          {{/each}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>

<button id="printTableBtn" class="px-4 py-2 mt-4 bg-secondary-600 text-white font-medium rounded-lg hover:bg-secondary-700">
  Print Timesheets Summary
</button>



     
